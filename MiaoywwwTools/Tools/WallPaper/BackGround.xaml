<Window x:Class="MiaoywwwTools.Tools.WallPaper.BackGround"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        AllowsTransparency="True"
        Background="Transparent"
        ResizeMode="NoResize"
        WindowStyle="None" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <Grid>
            <Viewbox Stretch=" fill">
                <Canvas Width="1920" Height="1080">
                    <MediaElement x:Name="medMain" MediaEnded="medMain_MediaEnded" UnloadedBehavior="Manual" Stretch="UniformToFill"/>
                    <Label x:Name="labContent" Background="Transparent" Height="1098" Width="1920" Content="content" FontSize="80" Canvas.Top="-3" VerticalAlignment="Top" FontWeight="Normal" />
                </Canvas>
            </Viewbox>
        </Grid>
    </Grid>
</Window>