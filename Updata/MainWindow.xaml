<Window x:Class="updata.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:updata"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        Title="更新" Height="175" Width="395" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <ProgressBar x:Name="pgbar" HorizontalAlignment="Center" Height="60" VerticalAlignment="Top" Width="374" Foreground="#FF2C74F3" RenderTransformOrigin="0.5,0.5" Margin="0,85,0,0" BorderBrush="#FF1F1F1F" Background="White" BorderThickness="1,1,1,1" />
        <Label x:Name="Label_Content" Content="内容" Height="55" Margin="0,10,0,0" VerticalAlignment="Top" Width="320" RenderTransformOrigin="0.5,0.5" FontSize="20" FontFamily="Microsoft JhengHei UI" FontWeight="Bold" BorderBrush="{x:Null}" />
    </Grid>
</Window>
